class menu():
    def __init__(self):
        self.d={}
      
    
    def __setitem__(self,key,value):
        self.d[key]=value
    
    def __repr__(self):
        return repr(self.d)
    def __iter__(self):
        return self
    def __next__(self):
            return self.d.items()
            
#    def show(self):
#        print(self.d)
m=menu()
m["idly"]=10
m["vada"]=20
for i,j in m:
    print(i,j)
    break
    
    
output :
('idly', 10) ('vada', 20)
    
